{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      { "when": "uber.gear.sculk_axe", "model": { "type": "minecraft:model", "model": "uberserver:item/sculk_axe" } },
      { "when": "uber.gear.purified_sculk_axe", "model": { "type": "minecraft:model", "model": "uberserver:item/purified_sculk_axe" } }
    ],
    "fallback": { "type": "minecraft:model", "model": "minecraft:item/diamond_axe" }
  }
}
